<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
  <key>Label</key>
  <string>my.startup</string>
  <key>ProgramArguments</key>
  <array>
    <string>sh</string>
    <string>-c</string>
    <string>
	launchctl setenv PKG_CONFIG_PATH /opt/X11/lib/pkgconfig:/usr/local/lib/pkgconfig:/usr/local/lib/pkgconfig:
    </string>
  </array>
  <key>RunAtLoad</key>
  <true/>
</dict>
</plist>
